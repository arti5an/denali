<% lazy_load = !local_assigns[:lazy_load_start_index] || entry_counter >= lazy_load_start_index %>
<%= render partial: 'entries/entry_list_photo', object: entry, as: :entry, locals: { lazy_load: lazy_load } if entry.is_photo? %>
<%= render partial: 'entries/entry_list_text',  object: entry, as: :entry if entry.is_text? %>
