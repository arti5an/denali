<% content_for :meta do %>
  <% cache "entry/meta/#{@entry.id}/#{@entry.updated_at.to_i}" do %>
    <%= render partial: 'entries/entry_meta', locals: { entry: @entry } %>
  <% end %>
<% end %>

<% content_for :inline_css do %>
  <% cache "#{@app_version}/entry/styles" do %>
    <style><%= Rails.application.assets['entry.css'].to_s.html_safe %></style>
  <% end %>
<% end %>

<%= render partial: 'entries/entry', object: @entry %>
<%= render partial: 'entries/entry_pagination', object: @entry %>
<%= render partial: 'entries/related', object: @entry if @photoblog.show_related_entries? %>
