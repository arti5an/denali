<% content_for :head do %>
  <%= render partial: 'entries/entry/head', object: @entry, as: :entry %>
<% end %>

<% content_for :inline_css do %>
  <% cache "#{ENV['CACHE_VERSION']}/entry/styles" do %>
      <style><%= inline_asset "entry.css" %></style>
  <% end %>
<% end %>

<%= render partial: 'entries/entry/body', object: @entry, as: :entry %>
<%= render partial: 'entries/entry/related', object: @entry, as: :entry if @photoblog.show_related_entries? %>
<%= render partial: 'partials/elsewhere' %>
