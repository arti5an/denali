<%# version: 1 %>
<% content_for :head do %>
  <%= render partial: 'entries/entry/head', object: @entry, as: :entry %>
  <link rel="amphtml" href="<%= @entry.amp_url %>">
<% end %>

<% content_for :typekit do %>
  <%= render partial: 'partials/typekit', locals: { typekit_id: ENV['typekit_body_id'] } if ENV['typekit_body_id'].present? && @entry.body.present? %>
<% end %>

<%= render partial: 'entries/entry/photo', object: @entry, as: :entry if @entry.is_photo? %>
<%= render partial: 'entries/entry/text', object: @entry, as: :entry if @entry.is_text? %>
<%= render partial: 'entries/entry/pagination', object: @entry, as: :entry %>
<%= render partial: 'entries/entry/related', object: @entry, as: :entry if @photoblog.show_related_entries? && !@photoblog.lazy_load_related_entries? %>
<%= render partial: 'entries/entry/lazy_related', object: @entry, as: :entry if @photoblog.show_related_entries? && @photoblog.lazy_load_related_entries? %>
<%= render partial: 'partials/elsewhere' %>
