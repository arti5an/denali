<% content_for :link_tags do %>
  <%= render partial: 'entries/entry/link_tags', object: @entry, as: :entry %>
  <%= tag :link, rel: 'preload', as: 'style' href: stylesheet_url('application') %>
<% end %>

<% content_for :head do %>
  <%= render partial: 'entries/entry/head', object: @entry, as: :entry %>
<% end %>

<% content_for :inline_css do %>
  <%= render partial: 'partials/inline_styles', locals: { stylesheet: 'entry.css' } %>
<% end %>

<% content_for :javascript do %>
  <%= javascript_pack_tag 'entry', defer: true %>
<% end %>

<%= render partial: 'entries/entry/body', object: @entry, as: :entry %>
<%= render partial: 'entries/entry/related', object: @entry, as: :entry if @photoblog.show_related_entries? %>
<%= render partial: 'partials/elsewhere' %>
