<% cache [@photoblog, @page, @count] do %>
  <% entries = @entries.to_a %>
  <%= render partial: 'entries/grid/observer', object: entries.shift, as: :entry, locals: { page_url: @page == 1 ? entries_url(page: nil) : entries_url(page: @page) } %>
  <%= render partial: 'entries/grid/regular',
      collection: entries,
      as: :entry,
      cached: true %>
<% end %>
