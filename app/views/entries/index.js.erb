<% cache "entries/js/page/#{@page}/count/#{@count}/#{@photoblog.cache_key}" do %>
  <%= render partial: 'entries/entry_list_item',
      collection: @entries,
      as: :entry,
      locals: {
        page_url: @page == 1 ? entries_url(page: nil) : entries_url(page: @page),
        lazy_load: true,
        image_variant: 'entry_list'
      } %>
<% end %>
