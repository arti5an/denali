<% cache "entry/related/amp/#{entry.id}/#{entry.updated_at.to_i}", expires_in: 2.weeks do %>
  <aside role="complementary">
    <h2 class="entry__related">Other Photos You Might Like</h2>
    <ol class="entry-list entry-list--related">
      <%= render partial: 'entries/amp/entry_list_item',
                  collection: @related,
                  as: :entry %>
    </ol>
  </aside>
<% end %>
