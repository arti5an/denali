<% cache "entry/related/#{@entry.id}/#{@entry.updated_at.to_i}/#{@photoblog.id}/#{@photoblog.updated_at.to_i}" do %>
  <% unless @entry.related.empty? %>
    <h2 class="entry__related">You might also like</h2>
    <ol class="entry-list entry-list--related">
      <%= render partial: 'entries/entry_list_item', collection: @entry.related %>
    </ol>
  <% end %>
<% end %>
