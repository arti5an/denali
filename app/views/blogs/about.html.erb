<% content_for :head do %>
  <%= render partial: 'head' %>
<% end %>

<% content_for :javascript do %>
  <%= javascript_pack_tag 'entry', defer: true %>
<% end %>

<article class="entry">
  <div class="entry__body">
    <h2 class="entry__headline">About <%= @photoblog.name %></h2>
    <%= @photoblog.formatted_about %>
  </div>
</article>
<%= render partial: 'partials/elsewhere' %>
