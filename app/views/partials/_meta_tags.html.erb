<meta charset="UTF-8">
<meta name="viewport" content="width=device-width">
<%= @photoblog.additional_meta_tags.try(:html_safe) %>
<% if @photoblog.favicon.present? %>
<link href="<%= @photoblog.favicon_url(w: 96) %>" rel="icon" type="image/png">
<% end %>
<% if @photoblog.touch_icon.present? %>
  <link href="<%= @photoblog.touch_icon_url(w: 192) %>" rel="icon" sizes="192x192">
  <link href="<%= @photoblog.touch_icon_url(w: 180) %>" rel="apple-touch-icon" sizes="180x180">
  <link href="<%= @photoblog.touch_icon_url(w: 152) %>" rel="apple-touch-icon" sizes="152x152">
  <link href="<%= @photoblog.touch_icon_url(w: 120) %>" rel="apple-touch-icon" sizes="120x120">
  <link href="<%= @photoblog.touch_icon_url(w: 76) %>" rel="apple-touch-icon" sizes="76x76">
<% end %>
